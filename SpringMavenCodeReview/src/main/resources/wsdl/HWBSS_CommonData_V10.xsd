	<xs:complexType name="PaymentRelation">
		<xs:sequence>
			<xs:element name="PaymentRelationId" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The id of payment relation.
it is null when adding the relation and it must be filled when modifying/updating the payment relation.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="64"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ServiceType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Service type that can be paid by the account. it is defined in the BSS.
The "F" means all of the service type.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="32"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PaymentLimit" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="LimitUnit" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Limit unit type.
1:Money
2:Usage</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string"/>
							</xs:simpleType>
						</xs:element>
						<xs:element name="LimitPattern" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Limit pattern, it can be a amount or a percentage.
1:Amount
2:Percentage</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string"/>
							</xs:simpleType>
						</xs:element>
						<xs:element name="LimitValue" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Limit value of different limit pattern. if the "LimitPattern" is Percentage this value is a percentage, if the "LimitPattern" is amount this value is an absolute value.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="64"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="LimitMeasureUnit" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The unit of the "LimitValue"  whenever the limit unit is usage.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="10"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ValidPeriod" type="common:TimePeriod" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The valid period of the payment realtion.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
