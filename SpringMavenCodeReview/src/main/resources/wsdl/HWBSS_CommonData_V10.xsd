<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:common="http://www.huawei.com/bss/soaif/interface/common/" xmlns:ns1="http://www.huawei.com/bss/soaif/interface/common/" targetNamespace="http://www.huawei.com/bss/soaif/interface/common/" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!--simple type defination-->
	<xs:simpleType name="Date">
		<xs:annotation>
			<xs:documentation>Format is YYYYMMDD</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="8"/>
			<xs:pattern value="\d{4}[0|1]\d[0-3]\d"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DateTime">
		<xs:annotation>
			<xs:documentation>Format is YYYYMMDD24hhmmss.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="14"/>
			<xs:pattern value="\d{4}[0|1]\d[0-3]\d[0-2]\d[0-5]\d[0-5]\d"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Language">
		<xs:annotation>
			<xs:documentation>The language code is defined in interface specification document.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="Nationality">
		<xs:annotation>
			<xs:documentation>The nationality code is defined in interface specification document.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="Title">
