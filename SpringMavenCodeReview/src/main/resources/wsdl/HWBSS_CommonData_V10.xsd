	<!--complex type defination-->
	<xs:element name="ReqHeader">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Version" type="common:Version" default="1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Interface version.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="BusinessCode" type="common:BusinessCode" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Business operation code generated by request system. This Code is used to identify different business process.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="TransactionId" type="common:TransactionId">
					<xs:annotation>
						<xs:documentation>Unique ID of each transaction,
This is a unique numeric string provided by the calling system. It is used by NGBSS internally to identify duplicate requests. This ID must be unique for each transaction.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Channel" type="common:Channel" minOccurs="0">
					<xs:annotation>
						<xs:documentation>This parameter is used to indicate different sales channels that the system interacts with.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PartnerId" type="common:PartnerId" minOccurs="0">
					<xs:annotation>
						<xs:documentation>This parameter is used when a BSS operator have some independent business partners. It can be used to distinguish the different business partner.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="BrandId" type="common:BrandId" minOccurs="0">
					<xs:annotation>
						<xs:documentation>The brand ID is used to distinguish different brand of one business partner.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ReqTime" type="common:DateTime">
					<xs:annotation>
						<xs:documentation>The sending time of this request message.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="TimeFormat" type="common:TimeFormat" minOccurs="0">
					<xs:annotation>
						<xs:documentation>the time type and time zone information.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="AccessUser">
					<xs:annotation>
						<xs:documentation>User name for accessing the system.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="32"/>
							<xs:minLength value="1"/>
						</xs:restriction>
