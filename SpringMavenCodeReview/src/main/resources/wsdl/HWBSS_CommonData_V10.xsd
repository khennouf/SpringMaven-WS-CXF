					<xs:documentation>It's filled 0 in the first query, and then filled the totalRowNum returned in the next queries.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BeginRowNum">
				<xs:annotation>
					<xs:documentation>Number of the start row.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:pattern value="[0-9]{1,2}"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="FetchRowNum">
				<xs:annotation>
					<xs:documentation>Number of obtained rows starting from BeginRowNum.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:pattern value="[0-9]{1,2}"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
